SELECT block_id, sender_amount FROM cmorq-data.ltc.sender_april WHERE sender_amount > 0 ;
SELECT max(receiver_amount) FROM cmorq-data.ltc.receiver_april limit 600 ;
SELECT min(receiver_amount) FROM cmorq-data.ltc.receiver_april limit 600 ;
SELECT block_id, receiver_amount FROM cmorq-data.ltc.receiver_april WHERE receiver_amount > 0 AND type='transfer';
SELECT avg(receiver_amount ) FROM cmorq-data.ltc.receiver_april limit 600 ;
SELECT count(distinct block_hash ) FROM cmorq-data.ltc.sender_april limit 600 ;
SELECT block_id, receiver_asset FROM cmorq-data.ltc.receiver_april WHERE receiver_amount >0 OR receiver_amount  >2;
SELECT sender_amount receiver_amount FROM cmorq-data.ltc.sender_april INNER JOIN cmorq-data.ltc.receiver_april ON sender_april.block_id = receiver_april.block_id ;
SELECT sender_amount receiver_amount FROM cmorq-data.ltc.sender_april RIGHT JOIN cmorq-data.ltc.receiver_april ON sender_april.block_id = receiver_april.block_id ORDER BY receiver_april.receiver_entity ;
SELECT sender_amount receiver_amount FROM cmorq-data.ltc.sender_april INNER JOIN cmorq-data.ltc.receiver_april ON sender_april.block_id = receiver_april.block_id ;
SELECT sender_amount receiver_amount FROM cmorq-data.ltc.sender_april LEFT JOIN cmorq-data.ltc.receiver_april ON sender_april.block_id = receiver_april.block_id ORder BY sender_april. sender_entity ;
